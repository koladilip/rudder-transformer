[
  {
    "destKey": "name",
    "sourceKeys": ["event", "properties.event", "name", "properties.name"],
    "required": false,
    "metadata": {
      "defaultValue": "Screen-Viewed",
      "template": "Screen-Viewed-{{value}}"
    }
  },
  {
    "destKey": "sessionId",
    "sourceKeys": "properties.kustomerSessionId",
    "required": false
  },
  {
    "destKey": "trackingId",
    "sourceKeys": "properties.kustomerTrackingId",
    "required": false
  },
  {
    "destKey": "meta",
    "sourceKeys": "properties",
    "required": false,
    "metadata": {
      "excludes": ["kustomerSessionId", "kustomerTrackingId"]
    }
  }
]
