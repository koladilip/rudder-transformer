    "required": true,
    "metadata": {
      "defaultValue": "Web-Page-Viewed",
      "template": "Web-Page-Viewed-{{value}}"
    }
  },
  {
    "destKey": "sessionId",
    "sourceKeys": "properties.kustomer_session_id",
