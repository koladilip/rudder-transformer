[
  {
    "destKey": "eventUniqueId",
    "sourceKeys": [
      "userId",
      "context.traits.userId",
      "context.traits.id",
      "anonymousId"
    ],
    "required": true
  },
  {
    "destKey": "eventName",
    "sourceKeys": "name",
    "required": false,
    "metadata": {
      "defaultValue": "Screen View",
      "template": "Screen View {{value}}"
    }
  },
  {
    "destKey": "properties",
    "sourceKeys": "properties",
    "required": false
  },
  {
    "destKey": "eventTime",
    "sourceKeys": ["originalTimestamp", "timestamp"],
    "required": true,
    "metadata": {
      "type": "timestamp"
    }
  }
]
